{
  "name": "network-interceptor",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "build": "babel src -d dist --source-maps",
    "build:webpack": "webpack",
    "start": "npm run build:webpack && node dist/index.js",
    "package": "npm run package-macos-x64 && npm run package-macos-arm64 && npm run package-linux-x64 && npm run package-linux-arm64",
    "package-macos-x64": "pkg . -t node16-macos-x64 --output build/macos-x64/network-interceptor",
    "package-macos-arm64": "pkg . -t node16-macos-arm64 --output build/macos-arm64/network-interceptor",
    "package-linux-x64": "pkg . -t node16-linux-x64 --output build/linux-x64/network-interceptor",
    "package-linux-arm64": "pkg . -t node16-linux-arm64 --output build/linux-arm64/network-interceptor",
    "tar": "npm run tar-macos-x64 && npm run tar-macos-arm64 && npm run tar-linux-x64 && npm run tar-linux-arm64",
    "tar-macos-x64": "mkdir -p build/macos-x64 && tar -cvzf ./build/macos-x64/network-interceptor.tar.gz -C ./build/macos-x64/ network-interceptor",
    "tar-macos-arm64": "mkdir -p build/macos-arm64 && tar -cvzf ./build/macos-arm64/network-interceptor.tar.gz -C ./build/macos-arm64/ network-interceptor",
    "tar-linux-x64": "mkdir -p build/linux-x64 && tar -cvzf ./build/linux-x64/network-interceptor.tar.gz -C ./build/linux-x64/ network-interceptor",
    "tar-linux-arm64": "mkdir -p build/linux-arm64 && tar -cvzf ./build/linux-arm64/network-interceptor.tar.gz -C ./build/linux-arm64/ network-interceptor",
    "move-to-tap": "cp -R ./build /Users/gajkumar/Documents/Workspace/Personal/shell\\ script/homebrew-tap/",
    "all": "npm run build:webpack && npm run package && npm run tar && npm run move-to-tap",
    "test": "NETWORK_INTERCEPTOR_LOGS='' && jest"
  },
  "bin": {
    "network-interceptor": "./dist/main.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "files": [
    "dist/**/*.json"
  ],
  "dependencies": {
    "atob": "^2.1.2",
    "axios": "^1.4.0",
    "btoa": "^1.2.1",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2",
    "path-browserify": "^1.0.1",
    "prettier": "^3.0.2",
    "puppeteer": "^21.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "jest": "^29.6.4",
    "json-loader": "^0.5.7",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
